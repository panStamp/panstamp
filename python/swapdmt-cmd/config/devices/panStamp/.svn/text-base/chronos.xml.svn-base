  <config>
    <reg name="Calibration" id="11">
      <param name="Use metrics (1=Yes 0=No)" type="bin">
        <position>0</position>
        <size>1</size>
        <default>1</default>
      </param>
      <param name="Current temperature in Celsious (NN.D format)" type="num">
        <position>1</position>
        <size>2</size>
        <default>0</default>
        <verif>[-+]?[0-9]*\.[0-9]{1}</verif>
      </param>
      <param name="Current altitude in mts" type="num">
        <position>3</position>
        <size>2</size>
      <verif>^(\d*)$</verif>
      </param>
    </reg>
